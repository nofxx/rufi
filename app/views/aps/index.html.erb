<h1>Listando Pontos de acesso</h1>
<p>
	<%= link_to 'Novo ap', new_ap_path %>
</p>
<% table @aps, ['Essid', 'MAC', 'Enc', 'Key', 'Ip', 'Mask', 'Dhcp', 'Canal', 'PaÃ­s', 'Cidade', 'Estado'] do |ap| %>
  <tr>
    <td><%=h ap.essid %></td>
    <td><%=h ap.mac %></td>
    <td><%=h ap.enc %></td>
    <td><%=h ap.key %></td>
    <td><%=h ap.ip %></td>
    <td><%=h ap.mask %></td>
    <td><%=h ap.dhcp %></td>
    <td><%=h ap.channel %></td>
    <td><%=h ap.country %></td>
    <td><%=h ap.city %></td>
    <td><%=h ap.region %></td>
    <td class="actions">
    	<%= link_to 'Ver', ap %>
    	<%= link_to 'Editar', edit_ap_path(ap) %>
    	<%= link_to 'Apagar', ap, :confirm => 'Tem certeza?', :method => :delete, :class => 'destroy' %>
    </td>
  </tr>
<% end %>
